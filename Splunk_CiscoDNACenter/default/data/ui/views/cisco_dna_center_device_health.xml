<form version="1.1">
  <label>Cisco DNA Center Device Health</label>
  <fieldset submitButton="false" autoRun="true">
    <input type="time" token="timeRangePicker3">
      <label>Time Range</label>
      <default>
        <earliest>-24h@h</earliest>
        <latest>now</latest>
      </default>
    </input>
    <input type="dropdown" token="ciscodnachostDropdown" searchWhenChanged="true">
      <label>Cisco DNA Center Host</label>
      <fieldForLabel>cisco_dnac_host</fieldForLabel>
      <fieldForValue>cisco_dnac_host</fieldForValue>
      <search>
        <query>sourcetype="cisco:dnac:devicehealth" | dedup cisco_dnac_host keepevents=false | table cisco_dnac_host</query>
        <earliest>-24h@h</earliest>
        <latest>now</latest>
      </search>
      <selectFirstChoice>true</selectFirstChoice>
    </input>
  </fieldset>
